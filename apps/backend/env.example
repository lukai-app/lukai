# LukAI Backend API Environment Variables
# Copy this file to .env.local and fill in your values

# App Configuration
NODE_ENV=development
PORT=3001

# Database
DATABASE_URL=postgresql://user:pass@localhost:5432/lukai
POSTGRES_DATABASE=lukai
POSTGRES_HOST=localhost
POSTGRES_PASSWORD=your-password
POSTGRES_USER=your-username
POSTGRES_PRISMA_URL=postgresql://user:pass@localhost:5432/lukai
POSTGRES_URL=postgresql://user:pass@localhost:5432/lukai
POSTGRES_URL_NON_POOLING=postgresql://user:pass@localhost:5432/lukai

# Authentication & Security
JWT_SECRET=your-super-secret-jwt-key-here
API_KEY=your-api-key-here
ENCRYPTION_MASTER_KEY=your-encryption-key-here

# External APIs
OPENAI_API_KEY=your-openai-api-key
MISTRAL_API_KEY=your-mistral-api-key

# WhatsApp Integration
WHATSAPP_ADMIN_NUMBER=your-whatsapp-admin-number
WHATSAPP_WEBHOOK_TOKEN=your-webhook-token
WHATSAPP_ACCESS_TOKEN=your-access-token
WHATSAPP_SENDER_ID=your-sender-id

# Google Services
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3001/auth/google/callback

# Analytics & Monitoring
MIXPANEL_TOKEN=your-mixpanel-token
LANGFUSE_SECRET_KEY=your-langfuse-secret
LANGFUSE_PUBLIC_KEY=your-langfuse-public

# File Storage
BLOB_READ_WRITE_TOKEN=your-blob-token

# Redis & Caching
UPSTASH_REDIS_REST_URL=your-redis-url
UPSTASH_REDIS_REST_TOKEN=your-redis-token

# Payment Processing
LEMON_WEBHOOK_SIGNING_SECRET=your-lemon-secret
LEMONSQUEEZY_API_KEY=your-lemonsqueezy-key

# Task Queue
QSTASH_URL=your-qstash-url
QSTASH_TOKEN=your-qstash-token
QSTASH_CURRENT_SIGNING_KEY=your-current-key
QSTASH_NEXT_SIGNING_KEY=your-next-key

# Cron Jobs
CRON_SECRET=your-cron-secret

# Admin & Service Tokens
ADMIN_TOKEN=your-admin-token
AGENT_SERVICE_TOKEN=your-agent-token

# Client URLs
CLIENT_BASE_URL=http://localhost:3000
API_BASE_URL=http://localhost:3001 