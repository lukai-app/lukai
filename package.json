{
  "name": "lukai",
  "version": "1.0.0",
  "private": true,
  "description": "Open-source AI scheduling infrastructure",
  "keywords": [
    "ai",
    "scheduling",
    "open-source",
    "typescript",
    "nextjs",
    "react-native"
  ],
  "homepage": "https://lukai.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukai/lukai.git"
  },
  "license": "AGPL-3.0",
  "author": {
    "name": "LukAI",
    "email": "hello@lukai.com"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "ee/*"
  ],
  "scripts": {
    "build": "turbo run build --filter='!@lukai/mobile'",
    "build:all": "turbo run build",
    "build:web": "turbo run build --filter='@lukai/web'",
    "build:mobile": "turbo run build --filter='@lukai/mobile'",
    "dev": "turbo run dev",
    "dev:all": "turbo run dev",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules && cd apps/agent && poetry env remove python",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:migrate": "turbo run db:migrate",
    "db:generate": "turbo run db:generate",
    "install:all": "npm install && cd apps/agent && source .venv/bin/activate && poetry install",
    "agent:install": "cd apps/agent && poetry install",
    "agent:dev": "cd apps/agent && source .venv/bin/activate && poetry run uvicorn app.main:app --reload --port 8000"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.12",
    "@types/react": "^18.3.12",
    "concurrently": "^8.2.2",
    "prettier": "^3.0.0",
    "react": "^18.3.1",
    "turbo": "^2.5.5",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "packageManager": "npm@10.0.0"
}
